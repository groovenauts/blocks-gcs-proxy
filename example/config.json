{
  "command": {
    "dryrun": {{ env "BLOCKS_BATCH_DRYRUN" }}
  },
  "progress": {
    "topic": "{{ env "BLOCKS_BATCH_PROGRESS_TOPIC" }}",
  },
  "job": {
    "subscription": "{{ env "BLOCKS_BATCH_PUBSUB_SUBSCRIPTION" }}",
    "pull_interval": 60,
    "sustainer": {
      "delay": {{ env "BLOCKS_BATCH_SUSTAINER_DELAY" }},
      "interval": {{ env "BLOCKS_BATCH_SUSTAINER_INTERVAL" }}
    }
  }
}
